e} onChange={onchange}/>\n\n        <input \n        type='radio' name='type' value='personal' \n        checked={type=='personal'} onChange={onchange}/> personal {' '}\n\n        <input type='radio' name='type' value='professional'  \n             checked={type=='professional'} onChange={onchange}/> professional\n\n        <div><input type='submit' value={current ? 'Update contact': 'Add contact' }/></div>\n        {current && (\n          <button onClick={allClear}>Clear</button>\n        )}\n    </form>\n  )\n}\n\nexport default ContactForm;\n","/home/sijad/contact-manager/Contact-management/client/src/context/alert/alertContext.js",["123"],[],"/home/sijad/contact-manager/Contact-management/client/src/context/Types.js",[],[],"/home/sijad/contact-manager/Contact-management/client/src/context/contacts/contactContext.js",["124"],[],"/home/sijad/contact-manager/Contact-management/client/src/context/alert/alertReducer.js",["125"],[],"import { SET_ALERT,REMOVE_ALERT } from \"../Types\"\nconst alertReducer= (state,action)=>{\n\n    switch(action.type){\n        case SET_ALERT:\n            return [...state,...action.payload];\n        case REMOVE_ALERT:\n\n            return   state.filter(alert=> alert.id !== action.payload.id );\n            \n    }\n}\nexport default alertReducer;",["126","127"],"/home/sijad/contact-manager/Contact-management/client/src/context/contacts/contactReducer.js",["128"],[],"/home/sijad/contact-manager/Contact-management/client/src/context/auth/authContext.js",["129"],[],"/home/sijad/contact-manager/Contact-management/client/src/components/contacts/ContactItems.js",["130"],[],"/home/sijad/contact-manager/Contact-management/client/src/components/auth/Login.js",["131","132","133"],[],"/home/sijad/contact-manager/Contact-management/client/src/components/auth/Register.js",["134","135","136","137","138","139"],[],"\nimport React,{useState,useEffect,useContext} from \"react\";\nimport authContext  from \"../../context/auth/authContext\";\nimport A